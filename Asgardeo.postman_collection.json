				{
					"name": "list-users",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"if(pm.response.code == 200){",
									"    // Parse the response body",
									"    var jsonData = pm.response.json();",
									"",
									"    // Function to get user ID by email",
									"    function getUserIdByEmail(email) {",
									"        var users = jsonData.Resources;",
									"        for (var i = 0; i < users.length; i++) {",
									"            if (users[i].emails && users[i].emails.includes(email)) {",
									"                return users[i].id;",
									"            }",
									"        }",
									"        return null;",
									"    }",
									"",
									"    // Get the email from environment variables",
									"    var targetEmail = \"brianab@gmail.com\";",
									"",
									"    // Get the user ID for the target email",
									"    var userId = getUserIdByEmail(targetEmail);",
									"",
									"    // Check if user ID is found",
									"    if (userId) {",
									"        console.log(\"User ID for email \" + targetEmail + \" is: \" + userId);",
									"        pm.environment.set(\"userId\", userId);",
									"    } else {",
									"        console.log(\"User with email \" + targetEmail + \" not found.\");",
									"    }",
									"",
									"}",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
