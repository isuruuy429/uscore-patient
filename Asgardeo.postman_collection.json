					"name": "list-applications",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Parse the response body",
									"var jsonData = pm.response.json();",
									"",
									"// Function to get application ID by name",
									"function getAppIdByName(appName) {",
									"    var apps = jsonData.applications;",
									"    for (var i = 0; i < apps.length; i++) {",
									"        if (apps[i].name === appName) {",
									"            return apps[i].id;",
									"        }",
									"    }",
									"    return null;",
									"}",
									"",
									"// Get the application name from environment variables",
									"var applicationName = pm.environment.get(\"applicationName\");",
									"",
									"// Get the application ID for the target application name",
									"var appId = getAppIdByName(applicationName);",
									"",
									"// Check if application ID is found",
									"if (appId) {",
									"    console.log(\"Application ID for application name \" + applicationName + \" is: \" + appId);",
									"    pm.environment.set(\"applicationId\", appId);",
									"} else {",
									"    console.log(\"Application with name \" + applicationName + \" not found.\");",
									"}",
									""
								],
